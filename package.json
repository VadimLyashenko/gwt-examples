{
  "name": "gulp-webpack-template",
  "version": "0.0.1",
  "description": "Start template with Gulp and Webpack",
  "author": "Vadim Lyashenko",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "gulp && webpack serve --progress -c config/webpack.dev.js",
    "build": "gulp build --build",
    "sprite": "gulp sprite",
    "fonts": "gulp fonts --rewrite",
    "devbuild": "gulp build --build --no-webp",
    "el": "eslint .",
    "sl": "stylelint \"**/*.{scss,htm,html,ejs}\"",
    "hl": "htmlhint \"src/**/*.{htm,html,ejs}\""
  },
  "devDependencies": {
    "@html-eslint/eslint-plugin": "^0.17.2",
    "@html-eslint/parser": "^0.17.1",
    "@primer/css": "latest",
    "@primer/stylelint-config": "latest",
    "copy-webpack-plugin": "latest",
    "css-loader": "latest",
    "del": "latest",
    "eslint": "^8.38.0",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "latest",
    "gulp-fonter-fix": "latest",
    "gulp-group-css-media-queries": "latest",
    "gulp-if": "latest",
    "gulp-newer": "latest",
    "gulp-notify": "latest",
    "gulp-plumber": "latest",
    "gulp-rename": "latest",
    "gulp-svg-sprite": "latest",
    "gulp-ttf2woff2": "latest",
    "gulp-webp": "latest",
    "gulp-webpcss": "latest",
    "html-loader": "latest",
    "html-webpack-plugin": "latest",
    "htmlhint": "latest",
    "inputmask": "latest",
    "js-beautify": "latest",
